    "jsge": "1.5.11",
    "path": "^0.12.7",
    "react": "19.1.0",
    "react-dom": "19.1.0"
  },
  "devDependencies": {
    "@babel/core": "7.27.1",
    "@babel/preset-env": "7.27.2",
    "@babel/preset-react": "7.27.1",
    "babel-loader": "10.0.0",
    "css-loader": "^7.1.2",
    "dotenv": "^17.2.3",
    "style-loader": "^4.0.0",
    "webpack-cli": "6.0.1",
    "webpack-dev-server": "5.2.0"
