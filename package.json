{
  "name": "gh-rts",
  "version": "0.2.3",
  "description": "classic rts goblin/humans, jsge engine",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "webpack serve --mode=development",
    "build": "webpack --mode=development",
    "build-contract": "cd ./GameStoreContract && forge build",
    "deploy": "cd ./GameStoreContract && forge create ./src/Store.sol:Store --private-key xxx --rpc-url http://127.0.0.1:8545 --broadcast --constructor-args 0xf39Fd6e51aad88F6F4ce6aB8827279cffFb92266",
    "test-gas-cost": "cd ./GameStoreContract && forge test --gas-report", 
    "testnet": "anvil",
    "test": "cd ./GameStoreContract && forge test"
  },
  "keywords": [
    "rts",
    "jsge",
    "game"
  ],
